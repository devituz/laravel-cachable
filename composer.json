{
  "name": "devituz/laravel-cachable",
  "description": "Simple and dynamic caching trait for Laravel models (Redis compatible)",
  "keywords": ["laravel", "cache", "redis", "model", "trait", "devituz"],
  "license": "MIT",
  "authors": [
    {
      "name": "Devituz",
      "email": "shohbozbek.uz24@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Devituz\\LaravelCachable\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Devituz\\LaravelCachable\\LaravelCachableServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
